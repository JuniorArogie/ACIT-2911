    <style>
        .bgcolor{
            background: url("../bg5.jpg") no-repeat center;
            background-size: 100%;
        }
    </style>
</head>
<body class="bgcolor justify-content-lg-center">

{{> game_nav}}

<table class="table table-sm">
    <thead>
    <tr>
        <th scope="col">Rank</th>
        <th scope="col">Players</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <th scope="row">1</th>
        <td>{{position1}}</td>
    </tr>
    <tr>
        <th scope="row">2</th>
        <td>{{position2}}</td>
    </tr>
    <tr>
        <th scope="row">3</th>
        <td>{{position3}}</td>
    </tr>
    <tr>
        <th scope="row">4</th>
        <td>{{position4}}</td>
    </tr>
    <tr>
        <th scope="row">5</th>
        <td>{{position5}}</td>
    </tr>
    </tbody>
</table>


</body>
